<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0" />
    <title>Home Page</title>
    <link rel="stylesheet" href="/css/Navpar/style.css" asp-append-version="true">
    <link rel="stylesheet" href="/css/Navpar/bootstrap.css" asp-append-version="true">
    <link rel="stylesheet" href="https://cdnjs.cloudflare.com/ajax/libs/font-awesome/6.5.0/css/all.min.css" asp-append-version="true">
    <link rel="preconnect" href="https://fonts.googleapis.com" asp-append-version="true">
    <link rel="preconnect" href="https://fonts.gstatic.com" crossorigin asp-append-version="true">
    <link href="https://fonts.googleapis.com/css2?family=Cairo:wght@200..1000&family=Open+Sans:ital,wght@0,300..800;1,300..800&family=Roboto:ital,wght@0,100;0,300;0,400;0,500;0,700;0,900;1,100;1,300;1,400;1,500;1,700;1,900&display=swap" rel="stylesheet">
    <style>
        html, body {
            height: 100%;
            margin: 0;
            padding: 0;
            overflow-y: auto;  
        }

        main {
            min-height: 100vh;  
        }

    </style>
</head>
<body>
    

<nav class="me-5 ms-5 mt-3 navbar navbar-expand-lg">
    <div class="container-fluid">
            <a class="navbar-brand" href="/Home/EventCategories"><img class="logo" src="/Images/Logo.png" alt=""></a>
        <button class="navbar-toggler" type="button" data-bs-toggle="collapse" data-bs-target="#navbarNavAltMarkup">
            <span class="navbar-toggler-icon"></span>
        </button>
        <div class="collapse navbar-collapse justify-content-end" id="navbarNavAltMarkup">
            <ul class="navbar-nav me-5 ms-5">
                    <li class="nav-item pt-2 pb-2 me-4"><a class="nav-link active" href="/Home/Index">Home</a></li>
                    <li class="nav-item pt-2 pb-2 me-4"><a class="nav-link" href="/Home/EventCategories">Events</a></li>
                    <li class="nav-item pt-2 pb-2 me-4"><a class="nav-link" href="/Home/About">About</a></li>
                    <li class="nav-item pt-2 pb-2 me-4"><a class="nav-link" href="/Home/Contact">Contact</a></li>
            </ul>
        </div>
    </div>
</nav>


  
    <div>
        <main >
            @RenderBody()
        </main>
    </div>
    <script src="~/lib/jquery/dist/jquery.min.js"></script>
    <script src="/js/Navpar/bootstrap.js" asp-append-version="true"></script>
    <script src="/js/Navpar/main.js" asp-append-version="true"></script>
    @await RenderSectionAsync("Scripts", required: false)

</body>
</html>
